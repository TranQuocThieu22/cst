{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "align": [
      false,
      "parameters"
    ],
    "ban-types": [
      false,
      [
        "Object",
        "Use {} instead."
      ],
      [
        "String"
      ]
    ],
    "max-line-length": [
      false
    ]
  }
}